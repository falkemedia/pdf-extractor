{
    "name": "robin7331/pdf-extractor",
    "description": "This package automates the generation of an SQLite database that you can use to do a full-text search on a PDF.",
    "keywords": [
        "robin7331",
        "pdf-extractor",
        "pdf",
        "parse"
    ],
    "homepage": "https://github.com/robin7331/pdf-extractor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Robin Reiter",
            "email": "robin7331@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-imagick": "*",
        "ext-sqlite3": "*",
        "intervention/image": "^2.5",
        "spatie/pdf-to-text": "dev-dynamically-find-pdftotext-binary"

    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Robin7331\\PdfExtractor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robin7331\\PdfExtractor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/robin7331/pdf-to-text"
        }
    ]
}
