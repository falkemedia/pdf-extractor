{
    "name": "falkemedia/pdf-extractor",
    "description": "This package automates the generation of an SQLite database that you can use to do a full-text search on a PDF.",
    "keywords": [
        "falkemedia",
        "pdf-extractor",
        "pdf",
        "parse"
    ],
    "homepage": "https://github.com/falkemedia/pdf-extractor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Robin Reiter",
            "email": "r.reiter@falkemedia.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-imagick": "*",
        "ext-sqlite3": "*",
        "intervention/image": "^2.5",
        "spatie/pdf-to-text": "^1.3.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "falkemedia\\PdfExtractor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "falkemedia\\PdfExtractor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
